    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.10",
    "prettier": "3.3.2",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
