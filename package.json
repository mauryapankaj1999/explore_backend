{
  "name": "explore_backend",
  "version": "1.0.0",
  "engines": {
    "node": ">=20",
    "npm": ">=10",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_PATH=./dist node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier:write": "prettier --write .",
    "build": "tsc",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "cross-env NODE_PATH=./dist  node --enable-source-maps --watch dist/server.js",
    "dev:alt": "concurrently --names \"tsc,node\" -c \"blue,green\"  \"npm run dev:tsc\"  \"npm run dev:node\"",
    "dev": "nodemon -e ts --watch src  --exec \"npm run build && npm start\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.4.4",
    "morgan": "^1.10.0",
    "znv": "^0.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.5",
    "@types/morgan": "^1.9.9",
